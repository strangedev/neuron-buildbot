{
  "name": "nodemon-buildbot",
  "version": "1.3.0",
  "description": "Builds a zettelkasten that is stored in a git repo when triggered by a webhook.",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf build",
    "test": "npx roboter",
    "watch": "nodemon --legacy-watch",
    "build": "npm run clean && npx roboter build",
    "semantic-release": "semantic-release"
  },
