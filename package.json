  "license": "LGPL-3.0",
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@types/chai": "4.2.14",
    "@types/express": "4.17.9",
    "@types/node": "14.14.10",
    "@types/shelljs": "0.8.8",
    "@types/sinon": "9.0.9",
    "chai": "4.2.0",
    "nodemon": "2.0.6",
    "rimraf": "3.0.2",
    "roboter": "11.5.22",
    "semantic-release": "17.3.0",
    "semantic-release-docker": "2.2.0",
    "sinon": "9.2.1",
    "ts-node": "9.1.0"
  },
  "dependencies": {
    "defekt": "6.0.1",
    "express": "4.17.1",
    "isomorphic-git": "1.8.0",
