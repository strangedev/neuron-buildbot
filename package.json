    "@semantic-release/git": "^9.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/shelljs": "^0.8.8",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "roboter": "^11.5.20",
    "semantic-release": "^17.3.0",
    "semantic-release-docker": "^2.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
